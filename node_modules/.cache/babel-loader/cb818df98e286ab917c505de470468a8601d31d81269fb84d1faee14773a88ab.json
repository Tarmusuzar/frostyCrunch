n        <button @click=\"closeCartDialog\">Checkout </button>\r\n        </div>\r\n      </div>\r\n    </dialog>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      menu: [\r\n        {\r\n          title:'Frosted Bananas',\r\n          products:[\r\n                { id: 1, name: 'Milk Chocolate', description: ' The classic frozen banana', price: 19, image: 'classic.png' },\r\n                { id: 2, name: 'White Chocolate', description: 'Banana dipped in ', price: 19, image: 'best.png' },\r\n                { id: 2, name: 'Dark Chocolate', description: 'Drippy Salted Caramel ', price: 19, image: 'kids.png' },\r\n                { id: 2, name: 'Peanut Butter', description: 'Amazing White Chocolate ', price: 19, image: 'special.png' },\r\n          ]\r\n        },\r\n        \r\n      ],\r\n      cart: [],\r\n      isCartDialogOpen: false,\r\n    };\r\n  },\r\n  methods: {\r\n    addToCart(item) {\r\n      this.cart.push({ ...item, quantity: 1 });\r\n    },\r\n    openCartDialog() {\r\n      this.isCartDialogOpen = true;\r\n    },\r\n    closeCartDialog() {\r\n      this.isCartDialogOpen = false;\r\n    },\r\n    \r\n    deleteCartItem(index) {\r\n      this.cart.splice(index, 1);\r\n    },\r\n    increaseQuantity(index) {\r\n      this.cart[index].quantity++;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n  /* General Styles */\r\n  body {\r\n    font-family: 'Helvetica Neue', sans-serif;\r\n    margin: 0;\r\n    padding: 0;\r\n    background-color: #f8f8f8; /* Light gray background */\r\n    color: #333; /* Dark gray text */\r\n  }\r\n\r\n  ::-webkit-scrollbar {\r\n  width: 12px; /* Set the width of the scrollbar */\r\n}\r\n\r\n/* Handle (thumb) styles */\r\n::-webkit-scrollbar-thumb {\r\n  background-color: #ffcc00; /* Set the color of the scrollbar handle */\r\n  border-radius: 6px; /* Round the corners of the handle */\r\n}\r\n\r\n/* Track (the area on which the handle moves) styles */\r\n::-webkit-scrollbar-track {\r\n  background-color: #f1f1f1; /* Set the color of the scrollbar track */\r\n}\r\n\r\n/* Make the scrollbar buttons (arrows) transparent */\r\n::-webkit-scrollbar-button {\r\n  display: none;\r\n}\r\n\r\n  h2 {\r\n    color: #333; /* Dark gray heading */\r\n  }\r\n\r\n  /* Menu Styles */\r\n  .cart-icon {\r\n    position: fixed;\r\n    top: 20px;\r\n    right: 20px;\r\n    cursor: pointer;\r\n    color: #333; /* Dark gray cart icon */\r\n  }\r\n\r\n  .cart-icon i {\r\n    font-size: 2em;\r\n  }\r\n\r\n  .cart-counter {\r\n    background-color: #ffcc00; /* McDonald's yellow */\r\n    color: #333; /* Dark gray text */\r\n    border-radius: 50%;\r\n    padding: 5px 10px;\r\n    position: absolute;\r\n    top: -5px;\r\n    right: -5px;\r\n    font-size: 12px;\r\n  }\r\n\r\n  /* Menu List Styles */\r\n  .menu-list {\r\n    list-style: none;\r\n    padding: 0;\r\n    display: flex;\r\n    width: 100%;\r\n    overflow-x: scroll;\r\n    position: relative;\r\n    align-items: center;\r\n  }\r\n\r\n  .menu-item {\r\n    background-color: #fff;\r\n    border-radius: 8px;\r\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n    padding: 20px;\r\n    margin-bottom: 20px;\r\n    flex: 1;\r\n    min-width: 5rem;\r\n    display: flex;\r\n    min-height: 29rem;\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n\r\n  .menu-item img {\r\n    width: 8rem;\r\n    border-radius: 8px;\r\n  }\r\n\r\n  .item-details {\r\n    margin-top: 10px;\r\n  }\r\n\r\n  .price {\r\n    font-weight: bold;\r\n    color: #333; /* Dark gray price text */\r\n  }\r\n\r\n  button {\r\n    background-color: #4caf50; /* Green add to cart button */\r\n    color: #fff; /* White text */\r\n    padding: 8px 16px;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    font-size: 14px;\r\n    transition: background-color 0.3s ease;\r\n  }\r\n\r\n  button:hover {\r\n    background-color: #45a049; /* Darker green on hover */\r\n  }\r\n\r\n  /* Cart Dialog Styles */\r\n  dialog {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: rgba(0, 0, 0, 0.5);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  .cart-dialog {\r\n    background-color: #fff;\r\n    border-radius: 8px;\r\n    padding: 20px;\r\n    max-width: 600px;\r\n    width: 100%;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n    max-height: 90vh;\r\n    overflow-y: scroll;\r\n\r\n  }\r\n\r\n  .cart-item {\r\n    display: flex;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .cart-item img {\r\n    width: 80px;\r\n    height: 80px;\r\n    border-radius: 8px;\r\n    margin-right: 20px;\r\n  }\r\n\r\n  .cart-item-details {\r\n    flex-grow: 1;\r\n  }\r\n\r\n  .cart-item-controls {\r\n    display: flex;\r\n    gap: 10px;\r\n  }\r\n\r\n  /* Optional: Add transitions for smoother effects */\r\n  .cart-item,\r\n  button {\r\n    transition: background-color 0.3s ease;\r\n  }\r\n\r\n  .cart-item:hover,\r\n  button:hover {\r\n    background-color: #f8f5e7; /* Light yellow on hover */\r\n  }\r\n\r\n  .lastSection {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    margin-top: 20px;\r\n  }\r\n\r\n  .lastSection button {\r\n    background-color: #007bff; /* Blue checkout button */\r\n    color: #fff; /* White text */\r\n    padding: 10px 20px;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    font-size: 16px;\r\n    transition: background-color 0.3s ease;\r\n  }\r\n  h2{\r\n    margin: 1rem .4rem;\r\n  }\r\n\r\n  .lastSection button:hover {\r\n    background-color: #0056b3; /* Darker blue on hover */\r\n  }\r\n  li{\r\n    list-style-type: none;\r\n  }\r\n  .make{\r\n    margin: 1rem;\r\n  }\r\n  \r\n</style>\r\n"],"mappings":";;gEAEIA,mBAAA,CAGM;EAHDC,KAAsE,EAAtE;IAAA;IAAA;IAAA;EAAA;AAAsE,I,aACzED,mBAAA,CAAoB,YAAhB,aAAW,G,aACjBA,mBAAA,CAA2C;EAAnCE,KAAK,EAAC;AAAM,GAAC,eAAa,E;gEAIhCF,mBAAA,CAAoC;EA