{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      menu: [{\n        id: 1,\n        name: 'Classic Banana',\n        description: 'A classic frozen banana',\n        price: 3.99,\n        image: 'banana.jpg'\n      }, {\n        id: 2,\n        name: 'Chocolate Dipped Banana',\n        description: 'Banana dipped in rich chocolate',\n        price: 4.99,\n        image: 'chocolate_banana.jpg'\n      }],\n      cart: [],\n      isCartDialogOpen: false\n    };\n  },\n  computed: {\n    totalCartQuantity() {\n      return this.cart.reduce((total, item) => total + item.quantity, 0);\n    },\n    totalCartPrice() {\n      return this.cart.reduce((total, item) => total + item.totalPrice, 0);\n    }\n  },\n  methods: {\n    addToCart(item) {\n      const existingItem = this.cart.find(cartItem => cartItem.id === item.id);\n      if (existingItem) {\n        existingItem.quantity++;\n      } else {\n        this.cart.push({\n          ...item,\n          quantity: 1\n        });\n      }\n    },\n    openCartDialog() {\n      this.isCartDialogOpen = true;\n    },\n    closeCartDialog() {\n      this.isCartDialogOpen = false;\n    },\n    editCartItem(index) {\n      // Implement edit functionality\n      console.log('Edit item:', this.cart[index]);\n    },\n    deleteCartItem(index) {\n      this.cart.splice(index, 1);\n    },\n    increaseQuantity(index) {\n      this.cart[index].quantity++;\n    },\n    checkout() {\n      // Implement checkout functionality\n      console.log('Checkout:', this.cart);\n    },\n    leaveComment() {\n      // Implement leave comment functionality\n      console.log('Leave a comment');\n    }\n  }\n};","map":{"version":3,"names":["data","menu","id","name","description","price","image","cart","isCartDialogOpen","computed","totalCartQuantity","reduce","total","item","quantity","totalCartPrice","totalPrice","methods","addToCart","existingItem","find","cartItem","push","openCartDialog","closeCartDialog","editCartItem","index","console","log","deleteCartItem","splice","increaseQuantity","checkout","leaveComment"],"sources":["C:\\Users\\ADMIN\\OneDrive\\Desktop\\frosty\\src\\components\\MainMenu.vue"],"sourcesContent":["<template>\r\n  <section>\r\n    <h2>Menu</h2>\r\n    <div class=\"cart-icon\" @click=\"openCartDialog\">\r\n      <i class=\"fas fa-shopping-cart\"></i>\r\n      <span class=\"cart-counter\">{{ totalCartQuantity }}</span>\r\n    </div>\r\n    <ul class=\"menu-list\">\r\n      <li v-for=\"item in menu\" :key=\"item.id\">\r\n        <div class=\"menu-item\">\r\n          <img :src=\"'../assets/' + item.image\" alt=\"Banana\" />\r\n          <div class=\"item-details\">\r\n            <h3>{{ item.name }}</h3>\r\n            <p>{{ item.description }}</p>\r\n            <p class=\"price\">${{ item.price.toFixed(2) }}</p>\r\n            <button @click=\"addToCart(item)\">Add to Cart</button>\r\n          </div>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n\r\n    <dialog v-show=\"isCartDialogOpen\">\r\n      <div class=\"cart-dialog\">\r\n        <h2>Shopping Cart</h2>\r\n        <ul>\r\n          <li v-for=\"(cartItem, index) in cart\" :key=\"index\">\r\n            <div class=\"cart-item\">\r\n              <img :src=\"'../assets/' + cartItem.image\" alt=\"Cart Item\" />\r\n              <div class=\"cart-item-details\">\r\n                <h3>{{ cartItem.name }}</h3>\r\n                <p class=\"price\">${{ cartItem.totalPrice.toFixed(2) }}</p>\r\n                <div class=\"cart-item-controls\">\r\n                  <button @click=\"editCartItem(index)\">Edit</button>\r\n                  <button @click=\"deleteCartItem(index)\">Delete</button>\r\n                  <button @click=\"increaseQuantity(index)\">+</button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n        <div class=\"cart-summary\">\r\n          <p>Total Quantity: {{ totalCartQuantity }}</p>\r\n          <p>Total Price: ${{ totalCartPrice.toFixed(2) }}</p>\r\n        </div>\r\n        <div class=\"cart-actions\">\r\n          <button @click=\"checkout\">Checkout</button>\r\n          <button @click=\"leaveComment\">Leave a Comment</button>\r\n          <button @click=\"closeCartDialog\">Close</button>\r\n        </div>\r\n      </div>\r\n    </dialog>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      menu: [\r\n        { id: 1, name: 'Classic Banana', description: 'A classic frozen banana', price: 3.99, image: 'banana.jpg' },\r\n        { id: 2, name: 'Chocolate Dipped Banana', description: 'Banana dipped in rich chocolate', price: 4.99, image: 'chocolate_banana.jpg' },\r\n      ],\r\n      cart: [],\r\n      isCartDialogOpen: false,\r\n    };\r\n  },\r\n  computed: {\r\n    totalCartQuantity() {\r\n      return this.cart.reduce((total, item) => total + item.quantity, 0);\r\n    },\r\n    totalCartPrice() {\r\n      return this.cart.reduce((total, item) => total + item.totalPrice, 0);\r\n    },\r\n  },\r\n  methods: {\r\n    addToCart(item) {\r\n      const existingItem = this.cart.find(cartItem => cartItem.id === item.id);\r\n\r\n      if (existingItem) {\r\n        existingItem.quantity++;\r\n      } else {\r\n        this.cart.push({ ...item, quantity: 1 });\r\n      }\r\n    },\r\n    openCartDialog() {\r\n      this.isCartDialogOpen = true;\r\n    },\r\n    closeCartDialog() {\r\n      this.isCartDialogOpen = false;\r\n    },\r\n    editCartItem(index) {\r\n      // Implement edit functionality\r\n      console.log('Edit item:', this.cart[index]);\r\n    },\r\n    deleteCartItem(index) {\r\n      this.cart.splice(index, 1);\r\n    },\r\n    increaseQuantity(index) {\r\n      this.cart[index].quantity++;\r\n    },\r\n    checkout() {\r\n      // Implement checkout functionality\r\n      console.log('Checkout:', this.cart);\r\n    },\r\n    leaveComment() {\r\n      // Implement leave comment functionality\r\n      console.log('Leave a comment');\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* ... (previous styles remain unchanged) */\r\n\r\n.cart-summary {\r\n  margin-top: 20px;\r\n}\r\n\r\n.cart-actions {\r\n  margin-top: 20px;\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\n/* Optional: Add transitions for smoother effects */\r\n.cart-item-controls button {\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.cart-item-controls button:hover {\r\n  background-color: #f8f5e7; /* Light yellow on hover */\r\n}\r\n</style>\r\n"],"mappings":";AAuDA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CACJ;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,WAAW,EAAE,yBAAyB;QAAEC,KAAK,EAAE,IAAI;QAAEC,KAAK,EAAE;MAAa,CAAC,EAC3G;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,yBAAyB;QAAEC,WAAW,EAAE,iCAAiC;QAAEC,KAAK,EAAE,IAAI;QAAEC,KAAK,EAAE;MAAuB,CAAC,CACvI;MACDC,IAAI,EAAE,EAAE;MACRC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,iBAAiBA,CAAA,EAAG;MAClB,OAAO,IAAI,CAACH,IAAI,CAACI,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAKD,KAAI,GAAIC,IAAI,CAACC,QAAQ,EAAE,CAAC,CAAC;IACpE,CAAC;IACDC,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACR,IAAI,CAACI,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAKD,KAAI,GAAIC,IAAI,CAACG,UAAU,EAAE,CAAC,CAAC;IACtE;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACL,IAAI,EAAE;MACd,MAAMM,YAAW,GAAI,IAAI,CAACZ,IAAI,CAACa,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACnB,EAAC,KAAMW,IAAI,CAACX,EAAE,CAAC;MAExE,IAAIiB,YAAY,EAAE;QAChBA,YAAY,CAACL,QAAQ,EAAE;MACzB,OAAO;QACL,IAAI,CAACP,IAAI,CAACe,IAAI,CAAC;UAAE,GAAGT,IAAI;UAAEC,QAAQ,EAAE;QAAE,CAAC,CAAC;MAC1C;IACF,CAAC;IACDS,cAAcA,CAAA,EAAG;MACf,IAAI,CAACf,gBAAe,GAAI,IAAI;IAC9B,CAAC;IACDgB,eAAeA,CAAA,EAAG;MAChB,IAAI,CAAChB,gBAAe,GAAI,KAAK;IAC/B,CAAC;IACDiB,YAAYA,CAACC,KAAK,EAAE;MAClB;MACAC,OAAO,CAACC,GAAG,CAAC,YAAY,EAAE,IAAI,CAACrB,IAAI,CAACmB,KAAK,CAAC,CAAC;IAC7C,CAAC;IACDG,cAAcA,CAACH,KAAK,EAAE;MACpB,IAAI,CAACnB,IAAI,CAACuB,MAAM,CAACJ,KAAK,EAAE,CAAC,CAAC;IAC5B,CAAC;IACDK,gBAAgBA,CAACL,KAAK,EAAE;MACtB,IAAI,CAACnB,IAAI,CAACmB,KAAK,CAAC,CAAC