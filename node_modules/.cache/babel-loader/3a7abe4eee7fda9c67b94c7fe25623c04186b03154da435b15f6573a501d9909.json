lected Rating: {{ ratings[category] }}</p>\r\n      </div>\r\n    </div>\r\n\r\n    <button @click=\"submitRating\" class=\"submit-button\">Submit Rating</button>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      ratings: {\r\n        foodQuality: 0,\r\n        service: 0,\r\n      },\r\n      hoverRating: 0,\r\n\r\n    };\r\n  },\r\n  methods: {\r\n    getCategoryLabel(category) {\r\n      return category === 'foodQuality' ? 'Food Quality' : 'Service';\r\n    },\r\n    selectRating(category, rating) {\r\n      this.ratings[category] = rating;\r\n      this.hoverRating = 0; // Reset hover state after selection\r\n    },\r\n    hoverRating(category, rating) {\r\n      this.hoverRating = rating;\r\n    },\r\n    resetHover() {\r\n      this.hoverRating = 0;\r\n    },\r\n    submitRating() {\r\n      // Perform actions with the ratings (e.g., send to the server)\r\n      console.log('Submitted Ratings:', this.ratings);\r\n      // You can also add a thank you message or redirect the user to another page\r\n    },\r\n    getRatingClass(category) {\r\n      return {\r\n        'below-five': this.ratings[category] < 5,\r\n        'average': this.ratings[category] >= 5 && this.ratings[category] <= 7,\r\n        'excellent': this.ratings[category] > 7,\r\n      };\r\n    },\r\n    getRatingColor(rating) {\r\n      if (rating < 5) {\r\n        return 'red';\r\n      } else if (rating >= 5 && rating <= 7) {\r\n        return 'orange';\r\n      } else {\r\n        return 'green';\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.rate-your-visit-container {\r\n  max-width: 600px;\r\n  margin: 50px auto;\r\n  padding: 20px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 10px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.title {\r\n  text-align: center;\r\n  color: #333;\r\n  font-size: 24px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.rating-section {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: space-around;\r\n}\r\n\r\n.rating-option {\r\n  text-align: center;\r\n  flex: 1 0 45%;\r\n  margin: 10px;\r\n}\r\n\r\n.rating-label {\r\n  color: #333;\r\n  font-size: 18px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.star-rating {\r\n  font-size: 24px;\r\n  cursor: pointer;\r\n}\r\n\r\n.star-rating span {\r\n  margin: 0 5px;\r\n  color: #ccc; /* Unfilled star color */\r\n}\r\n\r\n.star-rating span.filled {\r\n  color: #ffcc00; /* Filled star color */\r\n}\r\n\r\n.star-rating span.star-below-five.filled {\r\n  color: red; /* Filled star color for below 5 */\r\n}\r\n\r\n.star-rating span.star-average.filled {\r\n  color: orange; /* Filled star color for 5 to 7 */\r\n}\r\n\r\n.star-rating span.star-excellent.filled {\r\n  color: green; /* Filled star color for above 7 */\r\n}\r\n\r\n.selected-rating {\r\n  font-size: 16px;\r\n  margin-top: 5px;\r\n}\r\n\r\n.submit-button {\r\n  background-color: #4caf50;\r\n  color: #fff;\r\n  padding: 15px 30px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-size: 18px;\r\n  margin-top: 20px;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.submit-button:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n@media screen and (max-width: 768px) {\r\n  .rating-option {\r\n    flex: 1 0 100%;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;EACOA,KAAK,EAAC;AAA2B;gEACpCC,mBAAA,CAAsC;EAAlCD,KAAK,EAAC;AAAO,GAAC,iBAAe;;EAE5BA,KAAK,EAAC;AAAgB;;EAEpBA,KAAK,EAAC;AAAc;;;uBAL7BE,mBAAA,CAqBM,OArBNC,UAqBM,GApBJC,UAAsC,EAEtCH,mBAAA,CAeM,OAfNI,UAeM,I,kBAdJH,mBAAA,CAaMI,SAAA,QAAAC,WAAA,CAbwCC,MAAM,CAACC,IAAI,CAACC,KAAA,CAAAC,OAAO,GAA/BC,QAAQ;yBAA1CV,mBAAA,CAaM;MAbDF,KAAK,EAAC,eAAe;MAA2Ca,GAAG,EAAED;QACxEX,mBAAA,CAA4D,KAA5Da,UAA4D,EAAAC,gBAAA,CAAjCC,QAAA,CAAAC,gBAAgB,CAACL,QAAQ,mBACpDX,mBAAA,CASM;MATDD,KAAK,EAAAkB,eAAA,EAAC,aAAa,EAASF,QAAA,CAAAG,cAAc,CAACP,QAAQ;MAAIQ,YAAU,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEN,QAAA,CAAAO,UAAA,IAAAP,QAAA,CAAAO,UAAA,IAAAD,IAAA,CAAU;MAAE,aAAW,EAAC;uBAC9FpB,mBAAA,CAOOI