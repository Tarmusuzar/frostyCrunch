{\r\n              if (typeof newItem[prop] === 'object' && Array.isArray(newItem[prop]) && prop === 'orderItems') {\r\n                const orderItemsArr = newItem[prop];\r\n                for (const item of orderItemsArr) {\r\n                  if (typeof item === 'object' ) {\r\n                    total += +item.totalPrice;\r\n                    this.total = total\r\n                  }\r\n                }\r\n              }\r\n            }\r\n            this.fetchNewOrders('new-orders')\r\n            this.isNewOrder = true\r\n            \r\n\r\n\r\n            this.showSingleOrder = true\r\n            // Trigger sound and pop-up notification\r\n            this.playSound()\r\n            this.showNotification(`New item added: ${newItem}`)\r\n        })\r\n      },\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  div {\r\n   margin: 0 auto;\r\n  }\r\n  \r\n  nav {\r\n    display: flex;\r\n    width: 100%;\r\n    background-color: #3498db;\r\n  }\r\n  \r\n  button {\r\n    flex-grow: 1;\r\n    padding: 15px;\r\n    cursor: pointer;\r\n    border: none;\r\n    background-color: #2980b9;\r\n    color: white;\r\n    font-size: 16px;\r\n    transition: background-color 0.3s ease-in-out;\r\n  }\r\n  \r\n  button.active {\r\n    background-color: #2c3e50;\r\n  }\r\n  \r\n  component {\r\n    margin-top: 20px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAWE,OAAOA,QAAO,MAAO,iBAAiB;AACtC,OAAOC,SAAQ,MAAO,kBAAkB;AAExC,eAAe;EACbC,UAAU,EAAE;IACVF,QAAQ;IACRC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,UAAU,CAAE;IAC/B,CAAC;EACH,CAAC;EACDC,OAAO,EAAC;IACJC,aAAaA,CAAA,EAAE;MAEfC,QAAQ,CAACC,aAAa,CAAC;QACfC,MAAM,EAAE,yCAAyC;QACjDC,UAAU,EAAE,kCAAkC;QAC9CC,WAAW,EAAE,sDAAsD;QACnEC,SAAS,EAAE,kBAAkB;QAC7BC,aAAa,EAAE,8BAA8B;QAC7CC,iBAAiB,EAAE,cAAc;QACjCC,KAAK,EAAE;MACf,CAAC;;MAED;MACA,MAAMC,EAAC,GAAIT,QAAQ,CAACU,QAAQ,CAAC,CAAC;MAC9BD,EAAE,CAACE,GAAG,CAAC,UAAU,CAAC,CAACC,EAAE,CAAC,aAAa,EAAEC,QAAO,IAAK;QAC7C,MAAMC,OAAM,GAAID,QAAQ,CAACE,GAAG,CAAC;QAC7B,IAAI