kground-color: #fff;\r\n    border-radius: 8px;\r\n    padding: 20px;\r\n    max-width: 600px;\r\n    width: 100%;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  }\r\n\r\n  .cart-item {\r\n    display: flex;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .cart-item img {\r\n    width: 80px;\r\n    height: 80px;\r\n    border-radius: 8px;\r\n    margin-right: 20px;\r\n  }\r\n\r\n  .cart-item-details {\r\n    flex-grow: 1;\r\n  }\r\n\r\n  .cart-item-controls {\r\n    display: flex;\r\n    gap: 10px;\r\n  }\r\n\r\n  /* Optional: Add transitions for smoother effects */\r\n  .cart-item,\r\n  button {\r\n    transition: background-color 0.3s ease;\r\n  }\r\n\r\n  .cart-item:hover,\r\n  button:hover {\r\n    background-color: #f8f5e7; /* Light yellow on hover */\r\n  }\r\n\r\n  .lastSection {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    margin-top: 20px;\r\n  }\r\n\r\n  .lastSection button {\r\n    background-color: #007bff; /* Blue checkout button */\r\n    color: #fff; /* White text */\r\n    padding: 10px 20px;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    font-size: 16px;\r\n    transition: background-color 0.3s ease;\r\n  }\r\n\r\n  .lastSection button:hover {\r\n    background-color: #0056b3; /* Darker blue on hover */\r\n  }\r\n\r\n  h2 {\r\n    margin-bottom: 20px; /* Add space below the heading */\r\n  }\r\n\r\n  .menu-list {\r\n    list-style: none;\r\n    padding: 0;\r\n    overflow-y: auto; /* Add vertical scroll for overflow */\r\n    max-height: 500px; /* Set a max height for the menu */\r\n  }\r\n\r\n  .menu-item {\r\n    background-color: #fff;\r\n    border-radius: 8px;\r\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n    padding: 20px;\r\n    margin-bottom: 20px;\r\n    transition: transform 0.3s ease; /* Add transition for smoother effects */\r\n  }\r\n\r\n  .menu-item:hover {\r\n    transform: scale(1.05); /* Scale up on hover */\r\n  }\r\n\r\n  .item-details {\r\n    margin-top: 10px;\r\n  }