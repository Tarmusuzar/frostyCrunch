\n    <dialog v-show=\"isCartDialogOpen\">\r\n      <div class=\"cart-dialog\">\r\n        <h2>Shopping Cart</h2>\r\n        <ul>\r\n          <li v-for=\"(cartItem, index) in cart\" :key=\"index\">\r\n            <div class=\"cart-item\">\r\n              <img :src=\"'../assets/' + cartItem.image\" alt=\"Cart Item\" />\r\n              <div class=\"cart-item-details\">\r\n                <h3>{{ cartItem.name }}</h3>\r\n                <p class=\"price\">${{ cartItem.price.toFixed(2) }}</p>\r\n                <div class=\"cart-item-controls\">\r\n                  <button @click=\"deleteCartItem(index)\">Delete</button>\r\n                  <button @click=\"increaseQuantity(index)\">+</button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n\r\n        <div class=\"lastSection\">\r\n          <button @click=\"closeCartDialog\">Close</button>\r\n        <button @click=\"closeCartDialog\">Checkout </button>\r\n        </div>\r\n      </div>\r\n    </dialog>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      menu: [\r\n        { id: 1, name: 'Classic Banana', description: 'A classic frozen banana', price: 3.99, image: 'banana.jpg' },\r\n        { id: 2, name: 'Chocolate Dipped Banana', description: 'Banana dipped in rich chocolate', price: 4.99, image: 'chocolate_banana.jpg' },\r\n      ],\r\n      cart: [],\r\n      isCartDialogOpen: false,\r\n    };\r\n  },\r\n  methods: {\r\n    addToCart(item) {\r\n      this.cart.push({ ...item, quantity: 1 });\r\n    },\r\n    openCartDialog() {\r\n      this.isCartDialogOpen = true;\r\n    },\r\n    closeCartDialog() {\r\n      this.isCartDialogOpen = false;\r\n    },\r\n    \r\n    deleteCartItem(index) {\r\n      this.cart.splice(index, 1);\r\n    },\r\n    increaseQuantity(index) {\r\n      this.cart[index].quantity++;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n  /* General Styles */\r\n  bo